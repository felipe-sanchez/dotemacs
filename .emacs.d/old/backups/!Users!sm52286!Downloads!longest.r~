.longest.prefix <- function(c) {
    return(Reduce(function(a, b) {
        if (class(a) == "character")
            a <- strsplit(a, '')[[1]]
        b <- strsplit(b, '')[[1]]
        ret <- c()

        for (i in 1:(max(length(a), length(b)))) {
            if (a[i] == b[i])
                ret <- c(ret, a[i])
            else
                break
        }

        return(ret)
    }, c)

}
